@using System.Web.Optimization
@model eUseControl.Web.Models.VillaViewModel

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    <title>Villa Agency | @ViewBag.Title</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Vendors/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="~/assets/css/fontawesome.css">
    <link rel="stylesheet" href="~/assets/css/templatemo-villa-agency.css">
    <link rel="stylesheet" href="~/assets/css/owl.css">
    <link rel="stylesheet" href="~/assets/css/animate.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />
</head>

<body>

    <!-- ***** Preloader Start ***** -->
    <div id="js-preloader" class="js-preloader">
        <div class="preloader-inner">
            <span class="dot"></span>
            <div class="dots">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- ***** Preloader End ***** -->

    <div class="sub-header">
        @Html.Partial("_SubHeader")
    </div>

    <header class="header-area header-sticky">
        @Html.Partial("_Header")
    </header>

    <div class="main-banner">
        @RenderBody()
        @Html.Partial("_MainBanner")
    </div>

    <div class="featured section">
        @Html.Partial("_Featured")
    </div>

    <div class="video section">
        @Html.Partial("_Video")
    </div>

    <div class="video-content">
        @Html.Partial("_VideoContent")
    </div>

    <div class="section best-deal">
        @Html.Partial("_BestDeal")
    </div>

    <div class="properties section">
        @Html.Partial("_Properties")
    </div>

    <div class="contact section">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 offset-lg-4">
                    <div class="section-heading text-center">
                        <h6>| Contact Us</h6>
                        <h2>Get In Touch With Our Agents</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="contact-content">
        @Html.Partial("_Contact")
    </div>

    <footer>
        @Html.Partial("_Footer")
    </footer>

    <script src="~/Vendors/jquery/jquery.min.js"></script>
    <script src="~/Vendors/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/js/isotope.min.js"></script>
    <script src="~/assets/js/owl-carousel.js"></script>
    <script src="~/assets/js/counter.js"></script>
    <script src="~/assets/js/custom.js"></script>

</body>
</html>
